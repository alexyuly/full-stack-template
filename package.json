{
  "name": "full-stack-template",
  "version": "1.0.0",
  "description": "A very generic template for a typical full-stack socket-based Node.js web app",
  "main": "server/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "eslint server && rm -rf server_dist && babel server -d server_dist && webpack",
    "start": "NODE_ENV=production && cd server_dist && node index.js",
    "debug": "NODE_ENV=debug && eslint server && cd server && nodemon index.js --exec babel-node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexyuly/full-stack-template.git"
  },
  "author": "Alex Yuly",
  "license": "MIT",
  "homepage": "https://github.com/alexyuly/full-stack-template",
  "engines": {
    "node": "4.2.6",
    "npm": "3.5.2"
  },
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-tape-runner": "^2.0.1",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.21.0",
    "immutable": "^3.8.1",
    "material-ui": "^0.15.1",
    "nodemon": "^1.9.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.11.0",
    "webpack-merge": "^0.14.0"
  }
}
