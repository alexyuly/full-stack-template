{
  "name": "full-stack-template",
  "version": "1.0.8",
  "description": "A modern Node full stack template using Webpack, React, Material-UI, GraphQL, Relay, Express",
  "main": "server/index.js",
  "scripts": {
    "build": "eslint server && npm run update-schema && rimraf server_dist && babel server -d server_dist && webpack --progress",
    "start": "npm run build && cd server_dist && node index.js",
    "start-debug": "eslint server && npm run update-schema && cd server && nodemon index.js --exec babel-node",
    "update-schema": "babel-node scripts/updateSchema.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexyuly/full-stack-template.git"
  },
  "author": "Alex Yuly",
  "license": "MIT",
  "homepage": "https://github.com/alexyuly/full-stack-template",
  "metadata": {
    "graphql": {
      "schema": "./server/data/schema.json"
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-relay-plugin": "^0.9.1",
    "express": "^4.14.0",
    "express-graphql": "^0.5.3",
    "graphql": "^0.6.0",
    "graphql-relay": "^0.4.2",
    "material-ui": "^0.15.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-relay": "^0.9.1",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-relay-plugin-loader": "^0.9.0",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.21.0",
    "nodemon": "^1.9.2",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.11.0",
    "webpack-merge": "^0.14.0"
  }
}
